<table>
{% for item in items %}
<TR>
  <TD class="c1"><IMG src="{{ item.imgURL }}"></TD>
  <TD class="c5"><SPAN>LEGO {{ item.setNumber }}</SPAN></TD>
  <TD class="c3">http://amazon.pl{{ item.detailPageLink }}</TD>
  <TD class="c4"><SPAN>{{ item.priceInfo.priceToPay.displayString }}</SPAN></TD>
</TR>
{% endfor %}
</table>